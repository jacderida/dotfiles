[tagging]
use_genre = false

[encoding]
lame_encoding_setting=-V0
flac_encoding_setting=-8
flac_decode_setting=-d

[directories]
mp3_releases_base_directory=/mnt/archive/music/sorted/compressed/releases
mp3_release_directories=Ambient -- Drone -- Experimental,Early Electronic -- Tape Manipulation,Electronic (By Artist),Electronic (By Label),Krautrock -- Psychedelic Rock -- Prog Rock,Hip Hop -- Old School Electro,Industrial,Jazz -- Exotica,Library -- Soundtracks (By Artist),Library -- Soundtracks (By Label),Rock
